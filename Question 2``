//Find the maximum consecutive 1's in an array of 0's and 1's.
//Example:
//a) 00110001001110 - Output :3 [Max num of consecutive 1's is 3]
//b) 1000010001 - Output :1 [Max num of consecutive 1's is 1]


var n = prompt("enter the no.");
var count = 0;
while n>0
n & (n<<1);
count = count + 1;
console.log(count)
